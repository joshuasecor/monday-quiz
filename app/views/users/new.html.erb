<h1>New User</h1>

<%= form_for :user do |f| %>
  <p>
    <%= f.label :username %><br>
    <%= f.text_field :username %>
  </p>
 
  <p>
    <%= f.label :email %><br>
    <%= f.text_area :email %>
  </p>

  <p>
    <%= f.label :dob %><br>
    <%= f.text_area :dob %>
  </p>  
 
  <p>
    <%= f.submit %>
  </p>
<% end %>